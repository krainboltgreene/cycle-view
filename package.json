{
  "name": "snabbdom-view",
  "version": "1.0.0",
  "description": "Infuse your snabbdom components with state",
  "keywords": [
    "snabbdom",
    "state",
    "views"
  ],
  "contributors": [
    {
      "name": "Kurtis Rainbolt-Greene",
      "email": "kurtis@rainbolt-greene.online",
      "url": "https://kurtis.rainbolt-greene.online"
    }
  ],
  "license": "ISC",
  "homepage": "https://github.com/krainboltgreene/snabbdom-view#readme",
  "repository": "github:krainboltgreene/snabbdom-view",
  "bugs": "https://github.com/krainboltgreene/snabbdom-view/issues",
  "main": "transpiled/source/index.js",
  "files": [
    "transpiled/source/"
  ],
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "NODE_ENV=test tap --100 './source/**/test.js'",
    "build": "babel './source/' -d './transpiled/'",
    "lint": "eslint './source/'",
    "check": "flow check",
    "ci": "npm run test && npm run lint && npm run check",
    "watch": "chokidar './source/' --command 'npm run ci --silent'"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "chokidar-cli": "1.2.0",
    "eslint": "3.19.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-promise": "3.5.0",
    "flow": "0.2.3",
    "flow-bin": "0.51.1",
    "flow-typed": "2.1.5",
    "nyc": "11.1.0",
    "snabbdom-helpers": "4.0.3",
    "tap": "10.7.2"
  },
  "dependencies": {
    "@unction/applicator": "2.3.0",
    "@unction/applicators": "2.3.0",
    "@unction/arrayify": "2.4.0",
    "@unction/flip": "2.3.0",
    "@unction/istype": "2.3.0",
    "@unction/mapvalues": "3.3.0",
    "@unction/sequence": "1.0.0"
  }
}
